/; cstr_len (~uint8 cstr) [int]
    int i = 0
    /; loop (cstr{i} !== 0) [i++] ;/
    return i
;/

/; cstr_eq (~uint8 a, b) [bool]
    int ln = cstr_len(a)
    /; if (ln !== cstr_len(b))
        return false
    ;/

    /; loop (int i = 0; i < ln) [i++]
        /; if (a{i} !== b{i})
            return false
        ;/
    ;/

    return true
;/