# No longer simple
:include "c_wrap_linux.tnsl"

{}uint8 str = "Hello copy!\n\0"

struct Copier {
    ~uint8 h8r
}

/; method Copier

    /; copy [Copier]
        Copier out
	out.h8r = self.h8r
	return out
    ;/

;/

/; main
    Copier a
    a.h8r = ~str{0}
    ~Copier b
    b = ~a
    Copier c = b`.copy()
    _printf(c.h8r)
    return 0
;/

